{
  "name": "@brainbot/web",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev":   "kill-port 4321 && cross-env NEXT_TELEMETRY_DISABLED=1 next dev -p 4321",
    "build": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "kill-port 4321 && cross-env NEXT_TELEMETRY_DISABLED=1 next start -p 4321",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --write .",
    "check": "npm run typecheck && npm run lint && npm run build"
  },
  "dependencies": {
    "autoprefixer": "10.4.19",
    "next": "14.2.4",
    "postcss": "8.4.39",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "kill-port": "^2.0.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
